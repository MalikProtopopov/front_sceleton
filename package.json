{
  "name": "mediannfrontadmin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "docker:build": "docker build --build-arg NEXT_PUBLIC_API_URL=https://api.mediann.dev --build-arg NEXT_PUBLIC_ADMIN_URL=https://admin.mediann.dev -t mediann-admin .",
    "docker:run": "docker run -p 3000:3000 --rm mediann-admin",
    "compose:dev": "docker compose -f docker-compose.dev.yml up",
    "compose:dev:build": "docker compose -f docker-compose.dev.yml up --build",
    "compose:dev:down": "docker compose -f docker-compose.dev.yml down",
    "compose:prod": "docker compose -f docker-compose.prod.yml up -d",
    "compose:prod:build": "docker compose -f docker-compose.prod.yml up -d --build",
    "compose:prod:down": "docker compose -f docker-compose.prod.yml down",
    "compose:prod:logs": "docker compose -f docker-compose.prod.yml logs -f admin",
    "deploy": "./scripts/deploy.sh",
    "deploy:build-only": "./scripts/deploy.sh --build-only"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.17",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tiptap/extension-highlight": "^3.15.3",
    "@tiptap/extension-image": "^3.15.3",
    "@tiptap/extension-link": "^3.15.3",
    "@tiptap/extension-placeholder": "^3.15.3",
    "@tiptap/extension-text-align": "^3.15.3",
    "@tiptap/extension-underline": "^3.15.3",
    "@tiptap/pm": "^3.15.3",
    "@tiptap/react": "^3.15.3",
    "@tiptap/starter-kit": "^3.15.3",
    "@types/dompurify": "^3.0.5",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.71.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
